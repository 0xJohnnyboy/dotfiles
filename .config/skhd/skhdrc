# Focus spaces 
ctrl - h: yabai -m space --focus prev
ctrl - l: yabai -m space --focus next

# Focus display
meh - 2: yabai -m display --focus 2
meh - 1: yabai -m display --focus 1

# Resize window
meh - l: yabai -m window --resize left:20:0 || yabai -m window --resize right:20:0
meh - h: yabai -m window --resize left:-20:0 || yabai -m window --resize right:-20:0

# Focus window
alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north 
alt - l : yabai -m window --focus east

# Manipulate window
alt - r : yabai -m space --rotate 90
alt - f : yabai -m window --toggle zoom-fullscreen
